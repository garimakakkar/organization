<table>
	<tr>
		<th> First Name </th>
		<th> Last Name </th>
		<th> Email </th>
		<th> Mobile </th>
	</tr>
<% @users.each do |user| %>
<tr>
	<td> <%= user.first_name %> </td>
	<td> <%= user.last_name %> </td>
	<td> <%= user.email %> </td>
	<td> <%= user.mobile %> </td>
</tr>
<% end %>
</table>